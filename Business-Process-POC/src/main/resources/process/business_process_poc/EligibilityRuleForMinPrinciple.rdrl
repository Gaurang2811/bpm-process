package process.business_process_poc;

import java.lang.Number;

rule "EligibilityRuleForMinPrinciple"
	dialect "mvel"
	ruleflow-group "validations"
	no-loop true
	when
		person : Person( amount < 1000.0 , isEligible == true , amount > 100000.0 || amount < 1000.0 )
	then
		person.setIsEligible( false );
end
